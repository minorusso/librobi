<% if @books.present? %>
    <% @books.each do |book| %>
        <div class="list">
        <%= book.title %><br>
        <%= link_to (image_tag(book.medium_image_url)), book.item_url %><br>   
        <%= book.author %><br>
        <%= book.item_caption %><br>      
        </div>
    <% end %>
<% end %>
