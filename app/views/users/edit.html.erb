<h1>プロフィール編集</h1>
<%= form_with model: @user, local:true do |f| %>
    <label class="form-label">名前</label>
    <%= f.text_field :name %>
    <label class="form-label">メールアドレス</label>
    <%= f.text_field :email %>
    <label class="form-label">プロフィール画像</label><br>

<%= f.submit '保存する', class: "btn btn-success" %>
<% end %>